#include<iostream>
using namespace std;
bool isPrime(int a);
int main(){
    int n;
    cout<<"Enter the value of n: ";
    cin>>n;
    if(isPrime(n))
    cout<<"Prime";
    else
    cout<<"Not prime";
    return 0;
}
bool isPrime(int n)
{
    for( int i=2; i<n/2; i++)
    {
        if(n%i==0)
        return false;
        else
        return true;
    }
}
