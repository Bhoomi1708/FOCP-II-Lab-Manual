/*QUESTION 27. Display the maximum price in a item price list*/
#include<iostream>
using namespace std;
int main(){
    int a[10];
    int i;
    cout<<"Enter the prices: ";
    for(i=0; i<10; i++)
    cin>>a[i];
    int max= a[0];
    for(i=1; i<10; i++){
        if(max<=a[i])
        max=a[i];
    }
    cout<<"Maximum price: "<<max;
    return 0;
}
