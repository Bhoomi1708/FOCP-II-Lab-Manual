/*Triangle type based on its sides*/
#include<iostream>
using namespace std;
int main(){
    int a, b, c;
    cout<<"Enter the three sides of the triangle: ";
    cin>>a>>b>>c;
    if(a==b && b==c)
    cout<<"Equilateral triangle";
    else if(a!=b && b!=c && c!=a)
    cout<<"Scalene triangle";
    else
    cout<<"Isosceles triangle";
    return 0;
}
