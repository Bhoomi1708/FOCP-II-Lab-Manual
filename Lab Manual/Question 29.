/*Display the minimum temperature for the month*/
#include<iostream>
int main(){
    int i, temp[30];
    std::cout<<"Enter temperatures: ";
    for(i=0; i<30; i++)
    std::cin>>temp[i];
    int min=temp[0];
    for(i=1; i<30; i++){
        if(min>= temp[i])
        min= temp[i];
    }
    std::cout<<"Minimum temperature for the month: "<<min;
    return 0;
}
