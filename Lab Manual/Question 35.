/*Implement a solution to store a 3Ã—3 matrix and compute its transpose.*/
#include<iostream>
int main(){
    int i, j;
    int a[3][3];
    std::cout<<"Enter the elements of the matrix: \n";
    for(i=0; i<3; i++){
        for(j=0; j<3; j++)
        std::cin>>a[i][j];
    }
    std::cout<<"\nThe original matrix is \n";
    for(i=0; i<3; i++){
        for(j=0; j<3; j++)
        std::cout<<a[i][j]<<" ";
        std::cout<<std::endl;
    }
    std::cout<<"\nThe transpose of the matrix is \n";
    for(j=0; j<3; j++){
        for(i=0; i<3; i++)
        std::cout<<a[i][j]<<" ";
        std::cout<<std::endl;
    }
    return 0;
}
