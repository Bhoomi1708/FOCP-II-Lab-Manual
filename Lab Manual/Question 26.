/*QUESTION 26. Compute the total and percentage, and display the result*/
#include<iostream>
using namespace std;
int main(){
    float marks[5];
    int i=0;
    float total;
    cout<<"Enter marks of 5 subjects: ";
    while(i<5){
        cin>>marks[i];
        total= total+ marks[i];
        i++;}
        float percentage= total/5;
        cout<<"Total: "<<total<<endl;
        cout<<"Percentage: "<<percentage;
    return 0;
}
